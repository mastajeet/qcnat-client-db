CREATE TABLE `family` ( `family_id` BIGINT NOT NULL AUTO_INCREMENT , `name` VARCHAR(255) NOT NULL , `tel_1` VARCHAR(15) NOT NULL , `tel_2` VARCHAR(15) NULL , `address` TEXT NOT NULL , `email` VARCHAR(255) NULL , `created_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , `deleted_at` DATETIME NULL , PRIMARY KEY (`family_id`)) ENGINE = MyISAM;